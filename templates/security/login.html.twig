{% extends 'page_default.html.twig' %}

{% block page_title %}Login{% endblock %}

{% block main %}
    <div class="block block--security-register security-user">
        <div class="block-content">
            {{ form_start(form) }}
            {{ form_rest(form) }}
            <div class="form-group text-center">
                <button type="submit" class="btn btn-default">Sign In <span
                            class="glyphicon glyphicon-log-in"></span></button>
            </div>
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}
